{
  "compilerOptions": {
      "outDir": "./dist",
      "sourceMap": true,
      "declaration": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "module": "commonjs",
      "target": "es5",
      "suppressImplicitAnyIndexErrors": true
  },
  "include": [
      "./index.ts"
  ]
}